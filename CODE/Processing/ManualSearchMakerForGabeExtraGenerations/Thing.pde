class Thing {
  String clusterId = "";
  String title = "";
  int citeCount = 0;
  int year = 0;
  boolean isValid = true;
  //
  Thing (String clusterId, String title, int citeCount, int year, boolean isValid) {
    this.clusterId = clusterId;
    this.title = title;
    this.citeCount = citeCount;
    this.year = year;
    this.isValid = isValid;
  } // end constructor
} // end class Thing


